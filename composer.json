{
  "name": "hollow3464/auco-helper",
  "description": "Auco API Helper",
  "type": "library",
  "license": "GPLv3",
  "version": "2.0.1",
  "autoload": {
    "psr-4": {
      "Hollow3464\\AucoHelper\\": "src/"
    }
  },
  "authors": [
    {
      "name": "David Chamorro",
      "email": "daved3464@gmail.com"
    }
  ],
  "scripts": {
    "lint": "vendor/bin/phpstan",
    "test": "vendor/bin/pest",
    "format": "vendor/bin/pint",
    "fmt": "composer format"
  },
  "require": {
    "php": ">=8.1",
    "psr/http-client": "^1.0.3",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.1"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.8",
    "laravel/pint": "^1.15.1",
    "pestphp/pest": "^2.34",
    "phpstan/phpstan": "^1.10.67",
    "vlucas/phpdotenv": "^5.6"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  }
}
